{
	"name": "dataflow load title from csv to db",
	"properties": {
		"type": "MappingDataFlow",
		"typeProperties": {
			"sources": [
				{
					"dataset": {
						"referenceName": "ds_lake_titlemaster",
						"type": "DatasetReference"
					},
					"name": "sourcelaketitle"
				}
			],
			"sinks": [
				{
					"dataset": {
						"referenceName": "ds_sqldb_titlemd",
						"type": "DatasetReference"
					},
					"name": "sinkdbtitle"
				}
			],
			"transformations": [],
			"scriptLines": [
				"source(output(",
				"          TitleCode as string,",
				"          TitleDescription as string",
				"     ),",
				"     allowSchemaDrift: true,",
				"     validateSchema: false,",
				"     ignoreNoFilesFound: false) ~> sourcelaketitle",
				"sourcelaketitle sink(allowSchemaDrift: true,",
				"     validateSchema: false,",
				"     input(",
				"          TitleCode as string,",
				"          TitleDescription as string",
				"     ),",
				"     deletable:false,",
				"     insertable:true,",
				"     updateable:false,",
				"     upsertable:false,",
				"     format: 'table',",
				"     skipDuplicateMapInputs: true,",
				"     skipDuplicateMapOutputs: true,",
				"     errorHandlingOption: 'stopOnFirstError') ~> sinkdbtitle"
			]
		}
	}
}